Algoritmo "consumo"

Var
    distancia, consumo , medio : real


Inicio
     escreva("Distancia percorrida: ")
     leia(distancia)
     escreva("Combustível gasto: ")
     leia(consumo)
     
     medio <- distancia / consumo

     escreva("Consumo Medio = ", medio:8:3)

Fimalgoritmo